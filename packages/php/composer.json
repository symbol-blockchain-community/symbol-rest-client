{
  "name": "symbol-blockchain-community/symbol-rest-client",
  "description": "OpenAPI Specification of catapult-rest",
  "keywords": [
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api",
    "symbol"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "symbol-blockchain-community",
      "email": "symboljapanforumcommunity@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "SymbolRestClient\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SymbolRestClient\\Test\\": "test/"
    }
  },
  "archive": {
    "exclude": [
      "vendor",
      "/.openapi-generator",
      "/phpunit.xml",
      "/phpunit.xml.dist",
      "/.gitignore",
      "/.openapi-generator.ignore",
      "/git_push.sh"
    ]
  }
}
